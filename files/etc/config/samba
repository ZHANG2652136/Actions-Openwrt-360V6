
config samba
	option name 'OpenWrt'
	option workgroup 'WORKGROUP'
	option description 'OpenWrt'
	option homes '1'
	option autoshare '1'

config sambashare
	option browseable 'yes'
	option name '共享网盘'
	option path '/mnt/sda5'
	option users 'root'
	option read_only 'no'
	option guest_ok 'no'
	option create_mask '0777'
	option dir_mask '0777'

